@model Flutter.Client.Models.UserViewModel
<div class="column is-half is-post-container">
@foreach ( string item in Model.Users){
    <form asp-controller="User" asp-action="LoggingIn" method="GET">
            <div class="field">
                <label class="label">@item</label>
                <div class="control">
                    <input class="input is-hidden" type="text" asp-for="UserName" placeholder="UserName" value=@item></input>
                </div>
            </div>
            <div>
                <label class="label">Password</label>
                <div class="control">
                    <input class="input" type="password" asp-for="Password" placeholder="Password"></input>
                </div>
            </div>
               <div class="field">
                <div class="control">
                    <button class="button is-Flutter" type="submit">Login</button>
                </div>
            </div>
        </form>
}
</div>
