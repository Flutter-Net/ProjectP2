<div class="columns is-centered" id="loginpage">
    <div class="column is-one-third is-post-container">
        <form>
            <div class="field">
                <label class="label">UserName</label>
                <div class="control">
                    <input class="input" placeholder="UserName" id="UserNameInput" required></input>
                </div>
            </div>
            <div>
                <label class="label">Password</label>
                <div class="control">
                    <input class="input"  placeholder="Password" type="password" id="PasswordInput"  required></input>
                </div>
            </div>
               <div class="field">
                <div class="control">
                    <button class="button is-Flutter" id="LoginBtn">Login</button>
                </div>
            </div>
        </form>
    </div>
